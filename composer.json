{
  "name": "pdfme/php-generator",
  "description": "Generate PDFs from pdfme JSON layouts using mPDF without HTML rendering.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "mpdf/mpdf": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Pdfme\\Generator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pdfme\\Generator\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
